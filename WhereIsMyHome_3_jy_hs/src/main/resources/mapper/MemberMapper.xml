<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.member.model.mapper.MemberMapper">

	<!-- <resultMap type="userDto" id="user"> <result column="id" property="id" 
		/> <result column="password" property="password" /> <result column="role" 
		property="role" /> </resultMap> -->

	<!-- select * from members where id="ssafy" and password="1234"; -->
	<select id="selectMember" parameterType="map" resultType="userDto">
		select * from
		members
		where id = #{id} and password = #{password}
	</select>

	<!-- insert into members values("ssafy", "1234", "ssafy@ssafy.com", "김싸피"); -->
	<insert id="insertMember" parameterType="map">
		insert into members
		values(#{id},#{password}, #{email}, #{name})
	</insert>

</mapper>
    